<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <TextView
        android:id="@+id/textViewTtitleTab3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elegantTextHeight="true"
        android:fontFamily="sans-serif-black"
        android:gravity="center_horizontal"
        android:textSize="18sp"
        android:text="@string/altimeter_config_tab3"
        />
    <TextView
        android:id="@+id/txtAltimeterName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewTtitleTab3"

        android:layout_alignParentLeft="true"
        android:layout_marginTop="28dp"
        android:text="@string/alti_name"
        />

    <TextView
        android:id="@+id/txtAltiNameValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/txtAltimeterName"
        android:text="qqqq"
        />
    <TextView
        android:id="@+id/txtAltimeterID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtAltimeterName"

        android:layout_alignParentLeft="true"
        android:layout_marginTop="28dp"
        android:text="AltiID"
        />



    <EditText
        android:id="@+id/editTxtAltiIDValue"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignBaseline="@+id/txtAltimeterID"
        android:inputType="number"
        android:singleLine="true"
        android:text="0"
        android:textAlignment="textEnd"
        android:textIsSelectable="false"
        android:textSize="13sp"
        />
    <TextView
        android:id="@+id/txtAltiUnit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="18dp"
        android:layout_below="@+id/txtAltimeterID"
        android:text="@string/units"
        />

    <Spinner
        android:id="@+id/spinnerUnit"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignBaseline="@+id/txtAltiUnit"
        android:spinnerMode="dropdown" />

    <TextView
        android:id="@+id/txtAltiUseTelemetryPort"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="18dp"
        android:layout_below="@+id/txtAltiUnit"
        android:text="@string/use_telemetryport"
        />

    <Spinner
        android:id="@+id/spinnerUseTelemetryPort"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignBaseline="@+id/txtAltiUseTelemetryPort"
        android:spinnerMode="dropdown" />

    <TextView
        android:id="@+id/txtBpMode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtAltiUseTelemetryPort"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="18dp"
        android:text="@string/bip_mode"
        />

    <Spinner
        android:id="@+id/spinnerBipMode"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignBaseline="@+id/txtBpMode"
        android:spinnerMode="dropdown" />

    <TextView
        android:id="@+id/txtBeepFrequency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/txtBpMode"
        android:layout_marginTop="18dp"
        android:text="@string/beep_freq"
        />

    <EditText
        android:id="@+id/editTxtBipFreq"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignBaseline="@+id/txtBeepFrequency"
        android:inputType="number"
        android:singleLine="true"
        android:text="100"

        android:textAlignment="textEnd"
        android:textIsSelectable="false"
        android:textSize="13sp" />

    <TextView
        android:id="@+id/txtBluetoothName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtBeepFrequency"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="18dp"
        android:text="Bluetooth Name"
        />

    <EditText
        android:id="@+id/editBluetoothName"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignBaseline="@+id/txtBluetoothName"
        android:inputType="text"
        android:singleLine="true"
        android:text="Rocket"
        android:textAlignment="textEnd"
        android:textIsSelectable="false"
        android:textSize="13sp" />

    <TextView
        android:id="@+id/txtServoStayOn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtBluetoothName"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="18dp"
        android:text="@string/servo_stay_on"
        />

    <Spinner
        android:id="@+id/spinnerServoStayOn"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignBaseline="@+id/txtServoStayOn"
        android:spinnerMode="dropdown" />

    <TextView
        android:id="@+id/txtServoSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtServoStayOn"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="18dp"
        android:text="Servo Switch"
        />

    <Spinner
        android:id="@+id/spinnerServoSwitch"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignBaseline="@+id/txtServoSwitch"
        android:spinnerMode="dropdown" />

</RelativeLayout>